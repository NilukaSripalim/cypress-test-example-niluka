{
  "name": "cypress-test-example-niluka",
  "version": "1.0.0",
  "description": "Example code for series of blog posts",
  "main": "index.js",
  "scripts": {
    "cy:run": "cypress run --env allure=true",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:clear": "rm -R -f allure-results && rm -R -f allure-report && rm -R -f cypress/screenshots && rm -R -f cypress/videos",
    "pretest": "npm run allure:clear",
    "test": "npm run cy:run || npm run posttest",
    "posttest": "npm run allure:report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NilukaSripalim/cypress-test-example-niluka.git"
  },
  "keywords": [
    "cypress"
  ],
  "author": "niluka-sripali",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NilukaSripalim/cypress-test-example-niluka/issues"
  },
  "homepage": "https://github.com/NilukaSripalim/cypress-test-example-niluka#readme",
  "devDependencies": {
    "cypress": "^7.1.0"
  },
  "dependencies": {
    "@shelex/cypress-allure-plugin": "^2.5.0",
    "allure-commandline": "^2.13.8"
  }
}
