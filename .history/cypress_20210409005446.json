{
  "screenshotOnRunFailure": true,
  "screenshotsFolder": "cypress/reports/mochareports/assets",
  "videosFolder": "cypress/reports/mochareports/assets",
  "reporter": "cypress-multi-reporters",
    "reporterOptions": {
        "reporterEnabled": "mochawesome , cypress-dashboard",
        "cypressDashboardReporterOptions": {
          "reportDir": "cypress/reports/cypress-dashboard",
          "isLocalDeployment": true,
          "dashboardTitle": "Niluka- Cypress- Test"
        },
        "mochawesomeReporterOptions": {
            "reportDir": "cypress/reports/mocha",
            "quite": true,
            "overwrite": false,
            "html": false,
            "json": true
        }
    }
}
